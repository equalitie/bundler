{
	"useProxy": false,
	"drop_user": "nobody",
	"drop_group": "nogroup",
	"proxyAddress": "http://localhost:9008",
	"listenAddress": "127.0.0.1",
	"listenPort": 9008,
	"followFirstRedirect": true,
	"followAllRedirects": false,
	"redirectLimit": 10,
	"logDir": ".",
	"htmlDir": ".",
	"cloneHeaders": [
		"referer",
		"cookie"
	],
	"spoofHeaders": {
		"Origin": "Bundler"
	},
	"remapsFile": "./remaps.json"
}
